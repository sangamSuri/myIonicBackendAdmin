<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>df</servlet-name>
        <servlet-class>df</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>uploadServlet</servlet-name>
        <servlet-class>uploadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>App</servlet-name>
        <servlet-class>App</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>uploadImg</servlet-name>
        <servlet-class>uploadImg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>textnotify</servlet-name>
        <servlet-class>textnotify</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>uploadResult</servlet-name>
        <servlet-class>uploadResult</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>df</servlet-name>
        <url-pattern>/df</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>uploadServlet</servlet-name>
        <url-pattern>/uploadServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>App</servlet-name>
        <url-pattern>/App</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>uploadImg</servlet-name>
        <url-pattern>/uploadImg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>textnotify</servlet-name>
        <url-pattern>/textnotify</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>uploadResult</servlet-name>
        <url-pattern>/uploadResult</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
